				</Grid.RowDefinitions>
				<TextBlock Grid.Column="0" Grid.Row="0" Text="input device:"/>
				<TextBlock Grid.Column="0" Grid.Row="1" Text="output device:"/>
				<ComboBox Grid.Column="1" Grid.Row="0" ItemsSource="{Binding Path=InputDevices, Converter={StaticResource ResourceKey=DeviceNames}}" SelectedItem="{Binding Path=InputDevice, Converter={StaticResource ResourceKey=DeviceInputName}}" IsEnabled="{Binding Path=IsRunning, Converter={StaticResource ResourceKey=GeneralBoolToBoolInverted}}"/>
				<ComboBox Grid.Column="1" Grid.Row="1" ItemsSource="{Binding Path=OutputDevices, Converter={StaticResource ResourceKey=DeviceNames}}" SelectedItem="{Binding Path=OutputDevice, Converter={StaticResource ResourceKey=DeviceOutputName}}" IsEnabled="{Binding Path=IsRunning, Converter={StaticResource ResourceKey=GeneralBoolToBoolInverted}}"/>
			</Grid>
			<TextBlock Grid.Column="2" Text="mode:"/>
			<UniformGrid Grid.Column="3" Rows="2">
				<RadioButton Content="record" IsChecked="{Binding Path=RecordMode}" Command="{Binding Path=RecordSelectedCommand}"/>
				<RadioButton Content="playback" IsChecked="{Binding Path=RecordMode, Converter={StaticResource ResourceKey=GeneralBoolToBoolInverted}}" Command="{Binding Path=PlaybackSelectedCommand}"/>
			</UniformGrid>
			<ToggleButton Grid.Column="4" IsChecked="{Binding Path=IsRunning}" Command="{Binding Path=RecordPlayCommand}">
				<ToggleButton.Content>
					<MultiBinding Converter="{StaticResource ResourceKey=DeviceButtonRecordPlay}">
						<Binding Path="RecordMode"/>
						<Binding Path="IsRunning"/>
					</MultiBinding>
				</ToggleButton.Content>
			</ToggleButton>
			<Button Grid.Column="5" Command="{Binding Path=ClearRestartCommand}">
				<Button.Content>
					<MultiBinding Converter="{StaticResource ResourceKey=DeviceButtonClearRestart}">
						<Binding Path="RecordMode"/>
						<Binding Path="IsRunning"/>
					</MultiBinding>
				</Button.Content>
			</Button>
			<TextBlock Grid.Column="6" Text="graph:"/>
			<UniformGrid Grid.Column="7" Rows="2">
				<RadioButton Content="linear" IsChecked="{Binding Path=LinearGraph}"/>
				<RadioButton Content="aligned" IsChecked="{Binding Path=LinearGraph, Converter={StaticResource ResourceKey=GeneralBoolToBoolInverted}}"/>
			</UniformGrid>
